{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "monorepo": true,
  "root": "apps/app",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/app/tsconfig.app.json"
  },
  "projects": {
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "modulea": {
      "type": "application",
      "root": "apps/modulea",
      "entryFile": "main",
      "sourceRoot": "apps/modulea/src",
      "compilerOptions": {
        "tsConfigPath": "apps/modulea/tsconfig.app.json"
      }
    },
    "liba": {
      "type": "library",
      "root": "libs/liba",
      "entryFile": "index",
      "sourceRoot": "libs/liba/src",
      "compilerOptions": {
        "tsConfigPath": "libs/liba/tsconfig.lib.json"
      }
    }
  }
}